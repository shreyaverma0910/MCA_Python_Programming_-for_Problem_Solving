    "students = [('Alice', 88), ('Bob', 92), ('Carol', 79)]\n",
    "\n",
    "# Find topper using max with lambda\n",
    "topper = max(students, key=lambda x: x[1])\n",
    "\n",
    "print(f\"The student with highest marks: {topper[0]} ({topper[1]})\")\n"
   ]
