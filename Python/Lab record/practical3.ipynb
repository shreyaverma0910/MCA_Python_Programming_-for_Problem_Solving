   "source": [
    "#A program to reverse a number\n",
    "\n",
    "num = int(input(\"Enter a number: \"))\n",
    "print(\"Enter a number:\",num)\n",
    "\n",
    "reverse_num = 0\n",
    "\n",
    "while num > 0:\n",
    "    digit = num % 10\n",
    "    reverse_num = reverse_num * 10 + digit\n",
    "    num //= 10\n",
    "\n",
    "print(\"Reversed number:\", reverse_num)"
   ]
  }
