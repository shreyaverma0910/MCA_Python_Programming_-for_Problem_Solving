   "source": [
    "#Program to count number of vowels in a string \n",
    "string = input(\"Enter a string: \")\n",
    "print(\"Enter a string:\",string)\n",
    "\n",
    "vowels = \"aeiouAEIOU\"\n",
    "count = 0\n",
    "\n",
    "for char in string:\n",
    "    if char in vowels:\n",
    "        count += 1\n",
    "\n",
    "print(\"Number of vowels:\", count)"
   ]
